<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.fh.general.ecom.basics.dao.GuideArticleDao">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="org.fh.general.ecom.basics.model.GuideArticle">
		<result column="id" property="id" />
		<result column="guide_id" property="guideId" />
		<result column="type" property="type" />
		<result column="information" property="information" />
		<result column="create_Date" property="createDate" />
		<result column="update_Date" property="updateDate" />
		<result column="del" property="del" />
		<result column="published" property="published" />
		<result column="img" property="img" />
		<result column="imgmob" property="imgmob" />
		<result column="summary" property="summary" />
		<result column="publish_date" property="publishDate" />
		<result column="branch_name" property="branchName" />
		<result column="branch" property="branch" />
	</resultMap>
	<select id="selectByTitle" parameterType="java.lang.String" resultMap="BaseResultMap">
		select * from tb_guide_article where title=#{title}

	</select>
</mapper>
