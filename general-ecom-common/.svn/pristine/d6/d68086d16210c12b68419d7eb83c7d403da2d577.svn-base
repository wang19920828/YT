    private String txStatus;
    /**
     * 类型 0，提现；1，退款
     */
    private String type;
    /**
     * 订单号
     */
    @TableField("order_no")
    private String orderNo;
    /**
     * 退款状态0，待退款；1已退款
     */
    @TableField("tk_status")
    private String tkStatus;
    /**
     * 退款类型0，筹资失败退款，1冷静期退款
     */
